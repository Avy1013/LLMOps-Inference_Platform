{
  "dashboard": {
    "id": null,
    "title": "LLMOps Platform Dashboard",
    "tags": ["llmops", "kong", "api"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Count per API Key",
        "type": "stat",
        "targets": [
          {
            "expr": "topk(10, sum(rate(kong_http_requests_total{consumer!=\"\"}[5m])) by (consumer))",
            "legendFormat": "{{consumer}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate by Consumer (5m)",
